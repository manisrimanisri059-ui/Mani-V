import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
       Scanner s=new Scanner(System.in);
        String str=s.nextLine();
        int a=s.nextInt();
        int b=s.nextInt();
        StringBuilder sb=new StringBuilder(str);
        sb.delete(a,b);
        System.out.println(sb);
    }
}
