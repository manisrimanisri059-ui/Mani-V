import java.io.*;
import java.util.*;
import java.util.Scanner;
public class Main{
    public static void main(String[]args){
        Scanner sc=new Scanner(System.in);

        int N=sc.nextInt();  
        int largest=Integer.MIN_VALUE;
    
        for (int i=0;i<N;i++){
            int num=sc.nextInt();
            if (num>largest){
                largest=num; 
        }
       }
        System.out.println(largest);  
      }
  }
